{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Vehicle Dynamics Simulation Debugging",
            "preLaunchTask": "build_vehicle_dynamics_simulation_debug",
            "type": "cppdbg",
            "request": "launch",
            "args": ["--race", "vehicle_specifications/mazda_rx8_8HP70.yaml", "vehicle_specifications/mazda_rx8_8HP70_boosted.yaml"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "windows": {
                "type": "cppvsdbg",
                "program": "${workspaceFolder}/bazel-out/x64_windows-dbg/bin/vehicle_dynamics_simulation.exe",
            },
            "linux": {
                "type": "cppdbg",
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb",
                "program": "${workspaceFolder}/bazel-out/k8-dbg/bin/vehicle_dynamics_simulation",
            },
            "osx": {
                "type": "cppdbg",
                "MIMode": "lldb",
                "program": "${workspaceFolder}/bazel-out/darwin-dbg/bin/vehicle_dynamics_simulation",
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                }
            ]
        },
